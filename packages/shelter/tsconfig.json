{
  "include": ["src/**/*.ts", "src/**/*.tsx", "../../globals.d.ts"],
  "compilerOptions": {
    "target": "ESNext",
    "moduleResolution": "node",
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "noEmit": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "composite": true
  }
}
